{
  "manifest_version": 2,
  "name": "Torrent Control",
  "version": "0.1.0",
  "description": "Add torrent and magnet links to your Bittorrent client's web interface.",
  "default_locale": "en",
  "icons": {
    "32": "icon/default-32.png",
    "48": "icon/default-48.png",
    "64": "icon/default-64.png"
  },
  "options_ui": {
    "browser_style": true,
    "page": "view/options.html"
  },
  "permissions": [
    "menus",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "lib/baseclient.js",
      "lib/qbittorrent.js",
      "util.js",
      "index.js"
    ]
  }
}